#!/bin/bash
DIR="$(dirname $(readlink -f "$0"))"
pushd "$DIR"/../scripts > /dev/null
    . activate-juliawin-environment.sh
popd > /dev/null


if [ -f "$juliawin_vendor"/conda/Scripts/activate ]; then
    . "$juliawin_vendor"/conda/Scripts/activate
fi


if [ -f "$juliawin_vendor"/conda/python.exe ]; then 
    "$juliawin_vendor"/conda/python.exe "$@"
else
    "$juliawin_vendor"/conda/python "$@"
fi
exit $?